{
    "version": "1.0.0",
    "name": "lampminds/customization4",
    "description": "Filament-based customization components for Laravel projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lampminds\\Customization4\\": "src/"
        },
        "files": [
            "src/Filament/LmpCustomization/Helpers/helpers-common.php",
            "src/Filament/LmpCustomization/Helpers/helpers-page.php",
            "src/Filament/LmpCustomization/Helpers/helpers-parameters.php",
            "src/Filament/LmpCustomization/Helpers/helpers-log.php",
            "src/Filament/LmpCustomization/Helpers/helpers-useragent.php",
            "src/Filament/LmpCustomization/Helpers/helpers-links.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-permission": "^6.20"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lampminds\\Customization\\CustomizationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "filament/upgrade": "^4.0"
    }
}
